{
  "name": "setup-texlive-action",
  "private": true,
  "version": "2.6.2",
  "description": "A GitHub Action to set up TeX Live",
  "scripts": {
    "build": "npm run build:script",
    "build:script": "node scripts/build.mjs",
    "build:licenses": "webpack -c .config/webpack.config.mjs",
    "clean": "rimraf coverage dist lib *.tgz",
    "check": "tsc --noEmit",
    "dprint": "dprint -c .config/dprint.json",
    "fmt": "npm run dprint fmt",
    "fmt-check": "run-s -c 'fmt-check:*'",
    "fmt-check:dprint": "npm run dprint check",
    "fmt-check:ec": "git ls-files -z | xargs -0 ec",
    "link-check": "markdown-link-check README.md",
    "lint": "eslint -c .config/eslintrc.yml --ext .ts src tests",
    "test": "jest -c tests/jest.config.mjs --coverage",
    "pretest:e2e": "rimraf .cache/act",
    "test:e2e": "env XDG_CONFIG_DIRS=\"${PWD}/.config:${XDG_CONFIG_DIRS}\" act",
    "prepack": "run-s clean 'build:*'",
    "preversion": "npm run prepack && markdown-link-check dist/NOTICE.md",
    "version": "bash scripts/bump-version.sh"
  },
  "author": "@teatimeguest",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.mjs",
  "engines": {
    "node": ">=16"
  },
  "imports": {
    "##/*": "./*"
  },
  "files": [
    "action.yml",
    "dist"
  ],
  "dependencies": {
    "@abraham/reflection": "0.12.0",
    "@actions/cache": "3.2.2",
    "@actions/core": "1.10.1",
    "@actions/exec": "1.1.1",
    "@actions/http-client": "2.1.1",
    "@actions/io": "1.1.3",
    "@actions/tool-cache": "2.0.1",
    "class-transformer": "0.5.1",
    "semver": "7.5.4",
    "temporal-polyfill": "0.1.1",
    "typescript-memoize": "1.1.1",
    "unctx": "2.3.1"
  },
  "devDependencies": {
    "@types/jest": "29.5.3",
    "@types/node": "^16.18.38",
    "@types/semver": "7.5.0",
    "@typescript-eslint/eslint-plugin": "5.58.0",
    "@typescript-eslint/parser": "5.58.0",
    "better-typescript-lib": "2.4.0",
    "dprint": "0.39.0",
    "editorconfig-checker": "5.1.1",
    "esbuild": "0.18.11",
    "esbuild-loader": "3.0.1",
    "eslint": "8.44.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jest": "27.2.2",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-unicorn": "47.0.0",
    "git-cliff": "1.2.0",
    "jest": "29.6.1",
    "jest-extended": "4.0.0",
    "license-webpack-plugin": "4.0.2",
    "markdown-link-check": "3.11.2",
    "npm-run-all2": "6.0.6",
    "nunjucks": "3.2.4",
    "rimraf": "5.0.1",
    "ts-dedent": "2.2.0",
    "ts-essentials": "9.3.2",
    "ts-jest": "29.1.1",
    "tsconfig-paths-webpack-plugin": "4.0.1",
    "tsx": "3.12.7",
    "typescript": "5.1.6",
    "webpack": "5.88.1",
    "webpack-cli": "5.1.4"
  },
  "overrides": {
    "eslint-plugin-import": {
      "tsconfig-paths": "^4.1.1"
    }
  },
  "config": {
    "texlive": {
      "latest": {
        "version": "2023",
        "releaseDate": "2023-03-19T21:25:25.000Z"
      },
      "next": {
        "//": "See: https://www.tug.org/texlive/",
        "releaseDate": "2024-03-18"
      }
    }
  }
}
