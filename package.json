{
  "name": "setup-texlive-action",
  "private": true,
  "version": "2.6.3",
  "description": "A GitHub Action to set up TeX Live",
  "scripts": {
    "build": "npm run build:script",
    "build:script": "node scripts/build.mjs",
    "build:licenses": "webpack -c .config/webpack.config.mjs",
    "clean": "rimraf .cache coverage dist lib *.tgz",
    "check": "tsc --noEmit",
    "dprint": "dprint -c .config/dprint.json",
    "fmt": "npm run dprint fmt",
    "fmt-check": "run-s -c 'fmt-check:*'",
    "fmt-check:dprint": "npm run dprint check",
    "fmt-check:ec": "git ls-files -z | xargs -0 ec",
    "postinstall": "patch-package",
    "link-check": "markdown-link-check README.md",
    "lint": "eslint -c .config/eslintrc.yml --ext .ts src tests",
    "test": "jest -c tests/jest.config.mjs --coverage",
    "pretest:e2e": "rimraf .cache/act",
    "test:e2e": "cross-env XDG_CONFIG_DIRS=\"${npm_config_local_prefix}/tests\" act",
    "prepack": "run-s clean 'build:*'",
    "preversion": "bash scripts/bump-version.sh",
    "version": "bash scripts/bump-version.sh",
    "postversion": "bash scripts/bump-version.sh"
  },
  "author": "@teatimeguest",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.mjs",
  "engines": {
    "node": ">=20"
  },
  "imports": {
    "##/*": "./*"
  },
  "files": [
    "action.yml",
    "dist"
  ],
  "dependencies": {
    "@abraham/reflection": "0.12.0",
    "@actions/cache": "3.2.2",
    "@actions/core": "1.10.1",
    "@actions/exec": "1.1.1",
    "@actions/glob": "0.4.0",
    "@actions/http-client": "2.2.0",
    "@actions/io": "1.1.3",
    "@actions/tool-cache": "2.0.1",
    "class-transformer": "0.5.1",
    "core-js": "3.33.0",
    "deline": "1.0.4",
    "scule": "1.0.0",
    "semver": "7.5.4",
    "temporal-polyfill": "0.1.1",
    "ts-mixer": "6.0.3",
    "tslib": "2.6.2",
    "unctx": "2.3.1"
  },
  "devDependencies": {
    "@types/deline": "1.0.2",
    "@types/http-headers": "3.0.1",
    "@types/jest": "29.5.5",
    "@types/mock-fs": "4.13.2",
    "@types/node": "20.8.5",
    "@types/semver": "7.5.0",
    "@typescript-eslint/eslint-plugin": "6.7.4",
    "@typescript-eslint/parser": "6.7.4",
    "better-typescript-lib": "2.4.0",
    "cross-env": "7.0.3",
    "dprint": "0.41.0",
    "editorconfig-checker": "5.1.1",
    "esbuild": "0.19.4",
    "esbuild-loader": "4.0.2",
    "eslint": "8.51.0",
    "eslint-plugin-import": "2.28.1",
    "eslint-plugin-jest": "27.4.2",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-unicorn": "48.0.1",
    "git-cliff": "1.3.1",
    "http-headers": "3.0.2",
    "jest": "29.7.0",
    "jest-environment-node": "29.7.0",
    "jest-extended": "4.0.1",
    "license-webpack-plugin": "4.0.2",
    "markdown-link-check": "3.11.2",
    "mock-fs": "5.2.0",
    "nock": "13.3.3",
    "node-fetch": "2.7.0",
    "npm-run-all2": "6.1.1",
    "nunjucks": "3.2.4",
    "patch-package": "8.0.0",
    "rimraf": "5.0.5",
    "slugify": "1.6.6",
    "spdx-license-list": "6.7.0",
    "ts-dedent": "2.2.0",
    "ts-essentials": "9.3.2",
    "ts-jest": "29.1.1",
    "tsconfig-paths-webpack-plugin": "4.1.0",
    "tsx": "3.12.7",
    "typescript": "5.2.2",
    "webpack": "5.88.2",
    "webpack-cli": "5.1.4"
  },
  "overrides": {
    "eslint-plugin-import": {
      "tsconfig-paths": "^4.1.1"
    },
    "semver": "$semver"
  }
}
