{
  "name": "setup-texlive-action",
  "private": true,
  "version": "2.4.0",
  "description": "A GitHub Action to set up TeX Live",
  "scripts": {
    "build": "ttsc",
    "postbuild": "node .config/esbuild.config.mjs lib/index.js",
    "format": "dprint -c .config/dprint.json fmt",
    "format-check": "dprint -c .config/dprint.json check && ec --exclude=^dist",
    "link-check": "markdown-link-check README.md",
    "lint": "eslint -c .config/eslintrc.yml --ext .ts __tests__ src",
    "test": "jest -c .config/jest.config.mjs --coverage",
    "type-check": "ttsc --noEmit",
    "preversion": "npm run build && git add -u dist",
    "version": "bash scripts/bump-version.sh"
  },
  "author": "@teatimeguest",
  "license": "MIT",
  "main": "dist/index.js",
  "engines": {
    "node": ">=16"
  },
  "dependencies": {
    "@actions/cache": "3.0.4",
    "@actions/core": "1.10.0",
    "@actions/exec": "1.1.1",
    "@actions/glob": "0.3.0",
    "@actions/http-client": "2.0.1",
    "@actions/io": "1.1.2",
    "@actions/tool-cache": "2.0.1",
    "class-transformer": "0.5.1",
    "decorator-cache-getter": "1.0.0",
    "reflect-metadata": "0.1.13",
    "tslib": "2.4.0"
  },
  "devDependencies": {
    "@types/jest": "29.0.2",
    "@types/node": "18.7.18",
    "@typescript-eslint/eslint-plugin": "5.37.0",
    "@typescript-eslint/parser": "5.37.0",
    "dprint": "0.32.1",
    "editorconfig-checker": "4.0.2",
    "esbuild": "0.15.7",
    "eslint": "8.23.1",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jest": "27.0.4",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-unicorn": "43.0.2",
    "jest": "29.0.3",
    "jest-extended": "3.1.0",
    "markdown-link-check": "3.10.2",
    "ts-essentials": "9.3.0",
    "ts-jest": "29.0.1",
    "ts-transformer-keys": "0.4.3",
    "ttypescript": "1.5.13",
    "typescript": "4.8.3"
  }
}
