{
  "name": "setup-texlive-action",
  "private": true,
  "version": "2.2.2",
  "description": "A GitHub Action to set up TeX Live",
  "scripts": {
    "build": "npm run compile && npm run bundle",
    "bundle": "esbuild lib/index.js --bundle --platform=node --main-fields=module,main --outdir=dist --log-override:this-is-undefined-in-esm=debug",
    "compile": "ttsc",
    "format": "prettier --write {src,__tests__}/*.ts",
    "format-check": "prettier --check {src,__tests__}/*.ts && ec --exclude=^dist",
    "lint": "eslint --ext .ts __tests__ src",
    "postinstall": "patch-package",
    "release": "bash scripts/release.sh",
    "test": "jest --coverage",
    "type-check": "ttsc --noEmit"
  },
  "author": "@teatimeguest",
  "license": "MIT",
  "main": "lib/index.js",
  "engines": {
    "node": ">=16"
  },
  "dependencies": {
    "@actions/cache": "2.0.5",
    "@actions/core": "1.8.2",
    "@actions/exec": "1.1.1",
    "@actions/glob": "0.3.0",
    "@actions/io": "1.1.2",
    "@actions/tool-cache": "2.0.1",
    "class-transformer": "0.5.1",
    "decorator-cache-getter": "1.0.0",
    "reflect-metadata": "0.1.13"
  },
  "devDependencies": {
    "@types/jest": "28.1.0",
    "@types/node": "17.0.38",
    "@typescript-eslint/eslint-plugin": "5.27.0",
    "@typescript-eslint/parser": "5.27.0",
    "editorconfig-checker": "4.0.2",
    "esbuild": "0.14.42",
    "eslint": "8.16.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-jest": "26.4.6",
    "eslint-plugin-node": "11.1.0",
    "jest": "28.1.0",
    "jest-extended": "2.0.0",
    "patch-package": "6.4.7",
    "prettier": "2.6.2",
    "ts-essentials": "9.1.2",
    "ts-jest": "28.0.4",
    "ts-transformer-keys": "0.4.3",
    "ttypescript": "1.5.13",
    "typescript": "4.7.3",
    "typescript-transform-paths": "3.3.1"
  }
}
