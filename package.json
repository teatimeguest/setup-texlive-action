{
  "name": "setup-texlive-action",
  "private": true,
  "version": "2.1.1",
  "description": "A GitHub Action to set up TeX Live",
  "scripts": {
    "build": "ttsc && ncc build",
    "format": "prettier --write src/*.ts __tests__/*.ts",
    "format-check": "prettier --check __tests__/*.ts src/*.ts && ec",
    "lint": "eslint --ext .ts __tests__ src",
    "release": "bash scripts/release.sh",
    "test": "jest --coverage"
  },
  "author": "@teatimeguest",
  "license": "MIT",
  "main": "lib/index.js",
  "engines": {
    "node": ">=16"
  },
  "dependencies": {
    "@actions/cache": "2.0.2",
    "@actions/core": "1.7.0",
    "@actions/exec": "1.1.1",
    "@actions/glob": "0.3.0",
    "@actions/tool-cache": "1.7.2"
  },
  "devDependencies": {
    "@types/jest": "27.4.1",
    "@types/node": "17.0.30",
    "@typescript-eslint/eslint-plugin": "5.21.0",
    "@typescript-eslint/parser": "5.21.0",
    "@vercel/ncc": "0.33.4",
    "editorconfig-checker": "4.0.2",
    "eslint": "8.14.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-jest": "26.1.5",
    "jest": "27.5.1",
    "jest-circus": "27.5.1",
    "prettier": "2.6.2",
    "ts-jest": "27.1.4",
    "ts-transformer-keys": "0.4.3",
    "ttypescript": "1.5.13",
    "typescript": "4.6.4"
  }
}
