{
  "name": "setup-texlive-action",
  "private": true,
  "version": "2.2.1",
  "description": "A GitHub Action to set up TeX Live",
  "scripts": {
    "build": "ttsc && ncc build",
    "format": "prettier --write {src,__tests__}/*.ts",
    "format-check": "prettier --check {src,__tests__}/*.ts && ec --exclude=^dist",
    "lint": "eslint --ext .ts __tests__ src",
    "release": "bash scripts/release.sh",
    "test": "jest --coverage",
    "type-check": "ttsc --noEmit"
  },
  "author": "@teatimeguest",
  "license": "MIT",
  "main": "lib/index.js",
  "engines": {
    "node": ">=16"
  },
  "dependencies": {
    "@actions/cache": "2.0.5",
    "@actions/core": "1.8.2",
    "@actions/exec": "1.1.1",
    "@actions/glob": "0.3.0",
    "@actions/tool-cache": "2.0.1"
  },
  "devDependencies": {
    "@types/jest": "27.5.1",
    "@types/node": "17.0.35",
    "@typescript-eslint/eslint-plugin": "5.25.0",
    "@typescript-eslint/parser": "5.26.0",
    "@vercel/ncc": "0.33.4",
    "editorconfig-checker": "4.0.2",
    "eslint": "8.16.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-jest": "26.2.2",
    "eslint-plugin-node": "11.1.0",
    "jest": "28.1.0",
    "jest-circus": "28.1.0",
    "jest-extended": "2.0.0",
    "prettier": "2.6.2",
    "ts-essentials": "9.1.2",
    "ts-jest": "28.0.3",
    "ts-transformer-keys": "0.4.3",
    "ttypescript": "1.5.13",
    "typescript": "4.7.2",
    "typescript-transform-paths": "3.3.1"
  }
}
