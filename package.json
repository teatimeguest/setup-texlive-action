{
  "name": "setup-texlive-action",
  "private": true,
  "version": "2.4.1",
  "description": "A GitHub Action to set up TeX Live",
  "scripts": {
    "build": "node scripts/build.mjs",
    "build-release": "node scripts/build.mjs --release",
    "format": "dprint -c .config/dprint.json fmt",
    "format-check": "npm run format-check:dprint && npm run format-check:ec",
    "format-check:dprint": "dprint -c .config/dprint.json check",
    "format-check:ec": "git ls-files -z | xargs -0 ec --exclude=^dist",
    "link-check": "markdown-link-check README.md",
    "lint": "eslint -c .config/eslintrc.yml --ext .ts __tests__ src",
    "test": "jest -c .config/jest.config.mjs --coverage",
    "type-check": "ttsc --noEmit",
    "preversion": "npm run build-release",
    "version": "bash scripts/bump-version.sh"
  },
  "author": "@teatimeguest",
  "license": "MIT",
  "module": "dist/index.mjs",
  "engines": {
    "node": ">=16"
  },
  "dependencies": {
    "@actions/cache": "3.0.4",
    "@actions/core": "1.10.0",
    "@actions/exec": "1.1.1",
    "@actions/glob": "0.3.0",
    "@actions/http-client": "2.0.1",
    "@actions/io": "1.1.2",
    "@actions/tool-cache": "2.0.1",
    "class-transformer": "0.5.1",
    "decorator-cache-getter": "1.0.0",
    "reflect-metadata": "0.1.13",
    "tslib": "2.4.0"
  },
  "devDependencies": {
    "@types/dedent": "0.7.0",
    "@types/jest": "29.1.1",
    "@types/node": "18.8.2",
    "@typescript-eslint/eslint-plugin": "5.39.0",
    "@typescript-eslint/parser": "5.39.0",
    "dedent": "0.7.0",
    "dprint": "0.32.1",
    "editorconfig-checker": "4.0.2",
    "esbuild": "0.15.10",
    "eslint": "8.24.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jest": "27.1.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-unicorn": "44.0.1",
    "jest": "29.1.2",
    "jest-extended": "3.1.0",
    "license-webpack-plugin": "4.0.2",
    "markdown-link-check": "3.10.3",
    "ts-essentials": "9.3.0",
    "ts-jest": "29.0.3",
    "ts-transformer-keys": "0.4.3",
    "ttypescript": "1.5.13",
    "typescript": "4.8.4",
    "webpack": "5.74.0"
  }
}
