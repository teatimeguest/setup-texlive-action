{
  "name": "setup-texlive-action",
  "private": true,
  "version": "1.2.0",
  "description": "A GitHub Action to set up TeX Live",
  "scripts": {
    "build": "tsc && ncc build",
    "format": "prettier --write src/*.ts __tests__/*.ts",
    "format-check": "prettier --check __tests__/*.ts src/*.ts && ec",
    "lint": "eslint --ext .ts __tests__ src",
    "test": "jest --coverage"
  },
  "author": "@teatimeguest",
  "license": "MIT",
  "main": "lib/index.js",
  "dependencies": {
    "@actions/cache": "^1.0.8",
    "@actions/core": "^1.6.0",
    "@actions/exec": "^1.1.0",
    "@actions/glob": "^0.2.0",
    "@actions/tool-cache": "^1.7.1"
  },
  "devDependencies": {
    "@types/jest": "^27.0.3",
    "@types/node": "^16.11.11",
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^5.5.0",
    "@vercel/ncc": "^0.32.0",
    "editorconfig-checker": "^4.0.2",
    "eslint": "^8.3.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^25.3.0",
    "jest": "^27.4.2",
    "jest-circus": "^27.4.2",
    "prettier": "^2.5.0",
    "ts-jest": "^27.0.7",
    "typescript": "^4.5.2"
  }
}
